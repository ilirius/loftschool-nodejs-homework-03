extends _template

block content
  header.header(style="min-height: 7vh;")
    .container.header__container
      .header__top
        a(href="/").title-name Архипов
          span.title-name__doter .
        .header__social
          ul.social
            each item in social
              li.social__item
                a.social__link(href="http://")
                  i(class=`fab ${item}`)

  .container(style="min-height: 83vh;")
    h1 Error
    p= status
    p= error

  include ./common/_footer
